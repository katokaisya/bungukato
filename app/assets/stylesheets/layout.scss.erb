/*==アコーディオン==*/
.accordion {  padding:5px 10px; clear:both; }
.accordion label{
  display: block;
  margin-top: 5px;
  padding: 21px 5px;
  background: url(<%= asset_path('img/open.png') %>)no-repeat 95% center #f93;
  text-indent:1em;
  color: #fff;
  cursor: pointer;
  box-shadow: 1px 1px 2px rgba(3,2,0,0.8);
}
.accordion label img{ margin-right:10px;  vertical-align:-25%;}
.accordion input[type="checkbox"]{
  display: none;
}
.accordion .accordionIn {
  overflow: hidden;
  height: 0;
  text-decoration: none;
  -webkit-transition: height .5s ease-out;
  transition: height .5s ease-out;
}
.accordion input[type="checkbox"]:checked + .accordionIn{
  width:99.5%;
  display:block;
  height:100px;
  background-color:#fff;
  color: #666;
  border: 1px solid #f93;
  box-shadow: 1px 1px 2px rgba(3,2,0,0.8);
}
.accordion label img{ margin-right:10px;  vertical-align:-25%;}
.accordion label.active{
  background: url(<%= asset_path('img/close.png') %>)no-repeat 95% center #f93;
}
/*ボタン*/
#pageback,#pagetop {
  margin-top: 0;
  margin-bottom: 0;
  float: right;
}
#pageback a,#pagetop a {
  display: block;
  text-align: center;
  min-height: 16px;
  padding: 6px 10px;
}
.btn01 {
    font-size: 70%;
    color: #fff;
    text-decoration: none;
    background-color: rgba(255,255,255,0.5);
    display: block;
    height: 30px;
    line-height: 20px;
    border-radius: 5%;
    /* ここで動く速度とか設定 */
    transition: all 1s ease;
    -webkit-transition: all 1s ease;
}
.btn01:hover {
    transform: rotate(720deg);
    -webkit-transform: rotate(720deg);
}

/*===========ライトボックス風=============*/
#open01{
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    display: none;
    position: absolute;
}
.close_overlay {
    top: 0;
    left: 0;
    width: 100%;
    height: 100vh;
    display: block;
    text-indent: -9999px;
    position: absolute;
    background-color: #000;
    opacity: 0.5;
    z-index: 5;
}
.modal_window {
    top: 50%;
    left: 50%;
    margin: -150px 0 0 -200px;
    width: 400px;
    height: 300px;
    text-align: center;
    display: block;
    background: #fff;
    position: absolute;
    z-index: 10;
    border-radius: 5% 5%;
}
.modal_window h2 {  border-radius: 15% 15%; }

/* CSS3 ModalWindow SET
-------------------------- */
@-webkit-keyframes modalFadeIn {
    0% {opacity:0;display:block;}
    100% {opacity:0.9;}
}
div#modal div:target {
    -webkit-animation-name: modalFadeIn; 
    -webkit-animation-duration: 2s;
    -webkit-animation-iteration-count: 1;
    opacity: 0.9;
    display:block;
}



